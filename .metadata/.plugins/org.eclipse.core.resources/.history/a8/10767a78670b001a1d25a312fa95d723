import java.util.Scanner;

public class test {
	public static void main(String[] args) {
		Scanner scnScanner=new Scanner(System.in);
		String str0=scnScanner.next();
		String str1=scnScanner.next();
		int a=0;
		for(int i=1;i<=str0.length();i++)
		{
			if(str1.contains(str0.subSequence(i, i+1)))
			{
				a++;
			}
		}
			
		
		System.out.println(a);
		
	}
}
